{
  "name": "pre-claude",
  "version": "0.0.1-beta.1",
  "description": "üêç TUI for building structured prompts for Claude",
  "keywords": [
    "cli",
    "tui",
    "claude",
    "ai",
    "prompt",
    "structured"
  ],
  "author": "cut0",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/cut0/pre-claude.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "types": "./dist/types.d.ts",
  "main": "./dist/types.js",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "default": "./dist/types.js"
    }
  },
  "bin": {
    "pre-claude": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:types && pnpm build:cli",
    "build:cli": "node scripts/build-cli.ts",
    "build:types": "tsc src/types.ts --declaration --outDir dist --skipLibCheck --module ESNext --moduleResolution bundler --target ES2022",
    "dev": "pnpm build:cli && node dist/cli.js run --config examples/pre-claude-ja.config.ts",
    "lint:check": "biome check .",
    "lint:fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build",
    "release": "pnpm build && changeset publish"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "0.1.75",
    "@inkjs/ui": "2.0.0",
    "citty": "0.1.6",
    "consola": "3.4.2",
    "ink": "6.6.0",
    "ink-select-input": "6.2.0",
    "jiti": "2.6.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "valibot": "1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "2.28.1",
    "@types/node": "25.0.3",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "esbuild": "0.27.2",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.26.2"
}
